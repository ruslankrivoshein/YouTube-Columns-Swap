{
  "manifest_version": 2,
  "name": "YouTube Columns Swap",
  "version": "1.0",
  "description": "Swap YouTube columns for cases, when desktop panel is located at the left hand",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "YouTube Columns Swap",
    "default_popup": "popup/popup.html"
  }
}